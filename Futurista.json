{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#00FF00",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "foreground": "#00FF00",
          "powerline_symbol": " >",
          "properties": {
            "windows": "\ue62a ",
            "linux": "\uf17c ",
            "macos": "\uf179 ",
            "docker": "\uf308 ",
            "wsl": "\uf83c "
          },
          "style": "diamond",
          "template": " {{.Icon}}",
          "trailing_diamond": " |",
          "type": "os"
        },
        {
          "background": "",
          "foreground": "#00FFFF",
          "powerline_symbol": " | ",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "powerline",
          "template": "\uf07b {{ .Path }}",  
          "type": "path"
        },
        {
          "background": "",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF00FF{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FF6B6B{{ end }}",
            "{{ if gt .Ahead 0 }}#00FFFF{{ end }}",
            "{{ if gt .Behind 0 }}#FFA500{{ end }}"
          ],
          "foreground": "#00FF00",
          "powerline_symbol": " >",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "",
          "foreground": "#FF00FF",
          "style": "plain",
          "template": " \ue641 {{ .CurrentDate | date \"15:04:05\" }} ",
          "type": "time"
        },
        {
          "background": "",
          "foreground": "#00FFFF",
          "style": "plain",
          "template": "| {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB",
          "type": "sysinfo",
          "properties": {
            "display_memory": true
          }
        },
        {
          "background": "",
          "foreground": "#FFFF00",
          "style": "plain",
          "properties": {
            "always_enabled": true
          },
          "template": " | \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#00FF00",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#00FF00",
          "foreground_templates": ["{{ if gt .Code 0 }}#FF0000{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "$",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{.UserName}}@{{.HostName}} in {{ .PWD }}",
  "final_space": true,
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FF00FF",
    "template": "\u26A1 "
  },
  "version": 1
}
